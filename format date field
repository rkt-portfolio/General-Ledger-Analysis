
*FORMATTING DATE FIELDS :
DATA SASDATA.JE_&TYPE.;
SET JE_&TYPE.;
LOC_AMT = INPUT(COMPRESS(Amount_in_local_cur_,","),TRAILSGN32.);
EFF_DATE = INPUT(POSTED_ON,MMDDYY10.);
POST_DATE = INPUT(DOC__DATE,MMDDYY10.);
FORMAT EFF_DATE POST_DATE MMDDYY10.;
YEAR = "2011";
PERIOD = SUBSTR(COMPRESS(YEAR_MONTH),6,2);
RUN;
