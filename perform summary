/*JE SUMMARY CODE :*/
/* PERIOD WISE SUMMARY */
PROC SUMMARY DATA = SASDATA.JE NWAY MISSING;
CLASS PERIOD;
VAR LOC_AMT;
OUTPUT OUT = SASDATA.JE_PER_SUMM(DROP = _TYPE_ RENAME = (_FREQ_ = JE_COUNT)) SUM=;
RUN;


/*TB SUMMARY CODE */
/** TB ACCOUNT LEVEL SUMMARY **/

PROC SUMMARY DATA=SASDATA.TB_FINAL NWAY MISSING;
CLASS ACCOUNT;
VAR BEG_BAL END_BAL;
ID ACCT_DESCR;
OUTPUT OUT=SASDATA.TB_SUMM (DROP=_TYPE_ _FREQ_)SUM=;
RUN;
