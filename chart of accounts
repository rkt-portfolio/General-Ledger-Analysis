/** COA PREP ***/

   
	DATA SASDATA.COA_FINAL(KEEP=ACCT ACCT_DESC SUBTYPE);
	LENGTH SUBTYPE $30.;
	SET SASDATA.JE_ACCT_SUMM SASDATA.TB_SUMM;

	IF SUBSTR(COMPRESS(ACCT),1,1) = "1" THEN SUBTYPE="ASSET";
	ELSE IF SUBSTR(COMPRESS(ACCT),1,1) = "2" THEN SUBTYPE="LIABILITY";
	ELSE IF SUBSTR(COMPRESS(ACCT),1,1) = "3" THEN SUBTYPE="EQUITY";
	ELSE IF SUBSTR(COMPRESS(ACCT),1,1) = "4" THEN SUBTYPE="REVENUE";
	ELSE SUBTYPE="EXPENSE";

	RUN;

	PROC SORT DATA=SASDATA.COA_FINAL NODUPKEY OUT=SASDATA.COA_JEDAR;
	BY ACCT;
	RUN;

